{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "prod": {
      "root": "app",
      "sourceRoot": "app/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "app/dist/prod",
            "index": "app/src/index.html",
            "main": "app/src/index.ts",
            "tsConfig": "app/src/tsconfig.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "app/src/assets",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/css",
                "output": "/assets/css/font-awesome/css"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/webfonts",
                "output": "/assets/css/font-awesome/webfonts"
              },
              "app/src/WEB-INF/**"
            ],
            "styles": [],
            "scripts": []
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "prod:build"
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "prod:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "app/src/undefined",
            "karmaConfig": "karma.conf.js",
            "scripts": [],
            "styles": [],
            "assets": [
              {
                "glob": "**/*",
                "input": "app/src/assets",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/css",
                "output": "/assets/css/font-awesome/css"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/webfonts",
                "output": "/assets/css/font-awesome/webfonts"
              },
              "app/src/WEB-INF/**"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "prod-e2e": {
      "root": "app",
      "sourceRoot": "app/e2e",
      "projectType": "application"
    },
    "test": {
      "root": "",
      "sourceRoot": "test",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "test",
            "index": "test/index.html",
            "main": "test/main.ts",
            "tsConfig": "test/tsconfig.json",
            "assets": [],
            "styles": [],
            "scripts": []
          },
          "configurations": {}
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test:build"
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "test/test.ts",
            "karmaConfig": "karma.conf.js",
            "scripts": [],
            "styles": [],
            "assets": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "test-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application"
    },
    "dev": {
      "root": "app",
      "sourceRoot": "app/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "app/dist/dev",
            "index": "app/src/index.html",
            "main": "app/src/index.ts",
            "tsConfig": "app/src/tsconfig.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "app/src/assets",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/css",
                "output": "/assets/css/font-awesome/css"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/webfonts",
                "output": "/assets/css/font-awesome/webfonts"
              },
              "app/src/WEB-INF/**"
            ],
            "styles": [],
            "scripts": []
          },
          "configurations": {}
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dev:build"
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dev:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "app/src/undefined",
            "karmaConfig": "karma.conf.js",
            "scripts": [],
            "styles": [],
            "assets": [
              {
                "glob": "**/*",
                "input": "app/src/assets",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/css",
                "output": "/assets/css/font-awesome/css"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fortawesome/fontawesome-free/webfonts",
                "output": "/assets/css/font-awesome/webfonts"
              },
              "app/src/WEB-INF/**"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "dev-e2e": {
      "root": "app",
      "sourceRoot": "app/e2e",
      "projectType": "application"
    }
  },
  "defaultProject": "prod",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": ""
    }
  }
}